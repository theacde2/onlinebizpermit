{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.php",
            "use": "vercel-php@0.6.1"
        },
        {
            "src": "index.php",
            "use": "vercel-php@0.6.1"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.php"
        }
    ]
}
